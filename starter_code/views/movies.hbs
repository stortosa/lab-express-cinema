<h1>IronHack cinema</h1>
<div class="movies-titles">
  {{#each movies}}
  <h4>{{this.title}}</h4>
  <a href="/movie/{{this.id}}">See More</a>
  <img src='{{this.image}}' alt='' height="200" width="200">
  {{/each}}
</div>


